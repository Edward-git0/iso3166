{
    "name": "edward_y/iso3166",
    "description": "ISO 3166-1 PHP Library",
    "autoload": {
        "psr-4": { "edward_y\\ISO3166\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "edward_y\\ISO3166\\": "src" }
    },
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "cs-review": "php-cs-fixer fix --verbose --diff --dry-run",
        "cs-fix": "php-cs-fixer fix --verbose",
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": { "dev-master": "4.x-dev" }
    },
    "keywords": ["ISO 3166", "ISO", "3166", "3166-1", "countries", "library"],
    "homepage": "https://github.com/Edward-git0/iso3166",
    "license": "MIT",
    "authors": [{
        "name": "Edward Yarian",
        "email": "edward.email0@gmail.com"
    }],
    "support": {
        "issues": "https://github.com/Edward-git0/iso3166/issues",
        "source": "https://github.com/Edward-git0/iso3166"
    },
    "minimum-stability": "stable" 
}
